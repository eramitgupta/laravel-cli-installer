{
    "name": "erag/laravel-cli-installer",
    "description": "Laravel CLI tool to automate project setup and installation.",
    "keywords": [
        "laravel"

    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LaravelCliInstaller\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelCliInstaller\\LaravelCliInstallerServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/filesystem": "^11.0|^12.0",
        "illuminate/validation": "^11.0|^12.0",
        "laravel/prompts": "^0.3.7",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-mbstring": "*"
    },
    "scripts": {
        "lint": "pint",
        "lint-test": "pint --test"
    }
}
